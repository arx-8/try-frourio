{
  "name": "try-frourio",
  "version": "1.0.0",
  "scripts": {
    "dev:frourio": "cd src && frourio --watch",
    "dev:server": "tsx watch src/index.ts",
    "dev": "run-s generate && run-p dev:*",
    "eslint-print-config": "eslint --print-config src/index.ts > tmp/eslint-print-config.json",
    "generate:frourio": "cd src && frourio",
    "generate": "run-p generate:*",
    "lint-dry:eslint": "eslint -c .eslintrc.js --max-warnings=0 .",
    "lint-dry:format": "prettier --check .",
    "lint-dry:tsc": "tsc --noEmit",
    "lint-dry": "run-s --continue-on-error 'lint-dry:*'",
    "lint:eslint": "eslint -c .eslintrc.js --max-warnings=0 --fix .",
    "lint:format": "prettier --write .",
    "lint": "run-s --continue-on-error 'lint:*' lint-dry:tsc",
    "test-ci": "npm run generate && jest --ci",
    "test": "npm run generate && jest --watch"
  },
  "devDependencies": {
    "@jest/types": "29.3.1",
    "@swc/core": "1.3.26",
    "@swc/jest": "0.2.24",
    "@types/eslint": "8.4.10",
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "@types/prettier": "2.7.2",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-standard": "17.0.0",
    "eslint-interactive": "10.4.0",
    "eslint-plugin-array-func": "3.1.8",
    "eslint-plugin-import": "2.27.4",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-prefer-arrow-functions": "3.1.4",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "frourio": "0.31.1",
    "jest": "29.3.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "prettier-plugin-organize-imports": "3.2.2",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "fastify": "4.11.0",
    "tsx": "3.12.2"
  }
}
